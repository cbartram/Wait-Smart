{
  "name": "wait-smart",
  "version": "0.0.1",
  "description": "WaitSmart Backend serverless lambda function",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon test.js",
    "coverage": "CI=true NODE_ENV=test DYNAMODB_TABLE_NAME=table CLIENT_ID=foo CLIENT_SECRET=foo NO_CACHE=true nyc --reporter=lcovonly --reporter=text ./node_modules/.bin/mocha './tests/**/**.test.js' --timeout 10000 --exit",
    "build": "zip archive.zip -r -X ./index.js ./package.json ./node_modules ./src ./data",
    "deploy": "aws lambda update-function-code --function-name WaitSmart-Dev --zip-file fileb://archive.zip && rm archive.zip",
    "test": "NODE_ENV=test NO_CACHE=true ./node_modules/.bin/mocha './tests/**/**.test.js' --timeout 10000 --exit",
    "bump": "npm version patch --force",
    "tag": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbartram/Wait-Smart.git"
  },
  "keywords": [
    "wait",
    "smart",
    "javascript",
    "node",
    "serverless",
    "lambda",
    "api",
    "gateway",
    "universal"
  ],
  "author": "cbartram",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cbartram/Wait-Smart/issues"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true
    },
    "rules": {}
  },
  "eslintIgnore": [
    "client",
    ".idea",
    "data",
    "model",
    "node_modules",
    "pipeline"
  ],
  "homepage": "https://github.com/cbartram/Wait-Smart#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "mocha": "^7.0.1",
    "nock": "^13.0.5",
    "nyc": "^15.1.0",
    "sinon": "^9.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.820.0",
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "chart.js": "^2.9.3",
    "dotenv": "^8.2.0",
    "lambda-api-router": "^1.0.6",
    "moment": "^2.24.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  }
}
